{
  "expo": {
    "name": "phAMACore_App",
    "slug": "phAMACore_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher_round.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/launch_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pcico.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.netiapaul.phAMACore_App"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-secure-store",
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "3b980e33-ba3e-4d80-8849-bddbdd7d34df"
      }
    }
  }
}
